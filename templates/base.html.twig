<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Sortie-ENI{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
    {% block nav %}{% endblock %}

    {% block header %}
        {% for label, messages in app.flashes %}
            {% for message in messages %}
                <div class="flash flash-{{ label }}">
                    {{ message }}
                </div>
            {% endfor %}
        {% endfor %}

    {% endblock %}

    {% block main %}
        <div class="centre">
            <button class="btn-grad btn-grad1"> Annuler </button>
        </div>
    {% endblock %}
    {% block footer %}

        <div >
        <img src="{{ asset('image/logo-eni.png') }}" id="logo">
       </div>
        <!-- pied de page -->
        <div id="footer-author">
            <!-- auteur -->
           <br><a href="mailto:contact@eni-ecole.fr" title="Si vous dÃ©sirez nous Ã©crire">ENI ECOLE</a>
        </div>
        <div id="footer-copyright">
            <!-- copyright -->
           <br> <a href="http://www.eni-ecole.fr" title="Eni ECOLE " target="_blank">Copyright&copy; COOKIES TEAMS </a>
        </div>

    {% endblock %}

    </body>
</html>
